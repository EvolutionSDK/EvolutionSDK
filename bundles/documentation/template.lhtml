<!doctype html>
<html>
	<head>
		<title>@--title--@ &bull; EvolutionSDK&trade; Documentation</title>
		<style>
			body {
				background-color: #ccc;
				font-family: Palatino, "Palatino Linotype", "URW Palladio L", "Book Antiqua", Georgia, serif;
				line-height: 1.45;
				word-spacing: 0.05em;
			}
			.page {
				background: #fff;
				width: 960px;
				margin: 20px auto;
				box-shadow: 0 0 20px #000;
			}

			.nav-first {
				padding: 40px 40px 0;
				font-size: 125%;
				font-weight: bold;
				background: #eee;
				border-bottom: 1px solid #ccc;
			}

			.nav-first a {
				padding: 12px 16px 0;
				margin-bottom: -1px;
				color: #666;
				text-decoration: none;
				height: 34px;
				display: inline-block;
			}

			.nav-first a.selected {
				border: 1px solid #ccc;
				background-color: #fff;
				color: #222;
				border-bottom: none;
			}

			.nav-second {
				padding: 22px 40px 20px;
				line-height: 2em;
			}

			.nav-second a {
				color: #666;
				text-decoration: none;
				margin-right: 1.5em;
			}

			.nav-second a.selected {
				color: #222;
				border-bottom: 3px double #222;
			}

			.nav-second > *:first-child {
				margin-top: 0;
			}

			.nav-second > *:last-child {
				margin-bottom: 0;
			}

			.nav-third {
				padding: 21px 40px 20px;
				border: 1px solid #eee;
				border-left: none;
				border-right: none;
				background-color: #f8f8f8;
				font-size: 80%;
			}

			.nav-third a {
				color: #666;
				text-decoration: none;
				margin-right: 1.5em;
			}

			.nav-third a.selected {
				color: #222;
				border-bottom: 3px double #222;
			}

			.documentation {
				padding: 40px;
			}

			.documentation > *:first-child {
				margin-top: 0;
			}

			.documentation > *:last-child {
				margin-bottom: 0;
			}

			code {
				background-color: #F4F4F4;
				color: #736;
				padding: 0.25em 0.5em;
			}

			.documentation img {
				border: 2px solid #ccc;
				float: left;
				margin: 0 20px 10px 0;
			}

			.clear {
				clear: both;
			}

			.documentation dl dt {
				font-weight: bold;
				font-size: 110%;
			}

			.documentation dl dd {
				margin: 10px 10px 20px;
				color: #444;
			}

			.documentation hr {
				border: none;
				clear: both;
				padding: 0;
				margin: -1px 0 0;
				height: 1px;
			}

			.documentation.roles-and-responsibilities img {
				width: 128px;
				heigth: 128px;
			}
		</style>
		<script src="http://yandex.st/highlightjs/6.1/highlight.min.js"></script>
		<link rel="stylesheet" href="http://yandex.st/highlightjs/6.1/styles/magula.min.css" />
		<script>
			hljs.tabReplace = '    ';
			hljs.initHighlightingOnLoad();
		</script>
	</head>
	<body>
		<div class="page">
			<div class="nav-first">
				@--nav-first--@
			</div>
			<div class="nav-second">
				@--nav-second--@
			</div>
			<div class="nav-third">
				@--nav-third--@
			</div>
			<div class="documentation @--class--@">
				@--documentation--@
				<hr />
			</div>
		</div>
	</body>
</html>